    "start:debug": "npm run build:full && node scripts/start-debug.js",
    "start:prod": "node src/start-production.js",
    "start:cluster": "node src/start-production.js",
    "start:all": "npm run start:server",
    "dev:refresh": "node scripts/dev-refresh.js",
